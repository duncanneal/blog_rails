
<%= paginate @posts %>
    
<div class="row">
<% @posts.each do |post| %>
  <div class="post_wrapper">
    <h2 class="title"><%= link_to post.title, post%></h2>
    <p class="date"><%= post.created_at.strftime("%B %d, %Y") %></p>
    <h4 class="all_tags">#<%= link_to post.all_tags %></h5>
    <p class="pic"><%= image_tag(post.pic.thumb) %></p>
  </div>
<% end %>
</div>